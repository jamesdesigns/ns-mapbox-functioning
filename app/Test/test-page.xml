        <StackLayout>
            <Label textWrap="true" class="body m-20" text="Ogden Station"></Label>
            <Label textWrap="false" class="body m-20" text="2393 Wall Ave"></Label>
            <Label textWrap="false" class="body m-20" text="Ogden, UT 84020"></Label>
            <Label textWrap="true" class="body m-20" text="The following shows a few examples of how to use the NativeScript dialog module."></Label>
            <Button class="btn btn-primary" text="North Bound" tap="alertON"></Button>
            <Button class="btn btn-primary" text="South Bound" tap="alertOS"></Button>


            <!-- <Button class="btn btn-primary" text="Login" tap="login"></Button>
			<Button class="btn btn-primary" text="Action" tap="action"></Button> -->
